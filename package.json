        "@nestjs/core": "^8.0.0",
        "@nestjs/platform-express": "^8.0.0",
        "@nrwl/angular": "14.4.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.0.0",
        "tslib": "^2.0.0",
        "zone.js": "0.11.7"
    },
