        "@nx/cypress": "19.7.4",
        "@nx/eslint": "19.7.4",
        "@nx/eslint-plugin": "19.7.4",
        "@nx/jest": "19.7.4",
        "@nx/js": "19.7.4",
        "@nx/nest": "19.7.4",
        "@nx/node": "19.7.4",
